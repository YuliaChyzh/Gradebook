@model IEnumerable<PL.Models.SubjectViewModel>

@{
    ViewBag.Title = "ShowSubject";
}

<h2>ShowSubject</h2>

<table class="table">
    
    @foreach (var item in Model)
    {
    <tr>

        <td>
            <p>@item.Name</p>
        </td>
        <td>
            <p>@item.SubjectAvg</p>
        </td>
        <td>
            <p>@Html.ActionLink("Деталі", "SubjectDetails", "Subject", new { idSubject = item.Id }, null)</p>
        </td>
    </tr>
    }
</table>
