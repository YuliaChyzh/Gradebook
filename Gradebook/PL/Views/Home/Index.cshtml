@using BLL.DTO
@model Dictionary<PL.Models.StudentViewModel, GroupDTO>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">


    @foreach (var item in Model)
    {
        <tr>

            <td>
                <p>@item.Key.Name</p>
            </td>
            <td>
                <p>@item.Value.Name</p>
            </td>
        </tr>
    }



</table>